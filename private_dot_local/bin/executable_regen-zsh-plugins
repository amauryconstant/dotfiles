#!/usr/bin/env bash

# Regenerate Zsh Plugins - Direct CLI Access
# Purpose: Regenerate zsh plugins bundle
# Requirements: Arch Linux, zsh-antidote

# Use environment variables defined in shell/env
REGEN_SCRIPT="$SCRIPTS_DIR/terminal/regen-zsh-plugins.sh"

# Execute regen script
if [ -f "$REGEN_SCRIPT" ]; then
    "$REGEN_SCRIPT" "$@"
else
    echo "Error: Regenerate zsh plugins script not found at $REGEN_SCRIPT" >&2
    exit 1
fi
