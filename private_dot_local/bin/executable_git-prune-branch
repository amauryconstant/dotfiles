#!/usr/bin/env bash

# Git Prune Branch - Direct CLI Access
# Purpose: Prune git branches safely
# Requirements: Arch Linux, git

# Use environment variables defined in shell/env
GIT_PRUNE_SCRIPT="$SCRIPTS_DIR/git/prune-branch.sh"

# Execute git prune branch script
if [ -f "$GIT_PRUNE_SCRIPT" ]; then
    "$GIT_PRUNE_SCRIPT" "$@"
else
    echo "Error: Git prune branch script not found at $GIT_PRUNE_SCRIPT" >&2
    exit 1
fi