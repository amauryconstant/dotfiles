#!/usr/bin/env sh

# Unzip - Direct CLI Access
# Purpose: Wrapper around unar to provide unzip-compatible interface
# Requirements: unar (installed via packages.yaml)

# Use environment variables defined in shell/env
UNZIP_SCRIPT="$SCRIPTS_DIR/utils/unzip.sh"

# Execute unzip script
if [ -f "$UNZIP_SCRIPT" ]; then
    "$UNZIP_SCRIPT" "$@"
else
    echo "Error: Unzip script not found at $UNZIP_SCRIPT" >&2
    exit 1
fi
