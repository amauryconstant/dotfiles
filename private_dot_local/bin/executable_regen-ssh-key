#!/usr/bin/env sh

# CLI wrapper for SSH key regeneration
# Usage: regen-ssh-key <keyname>

REGEN_SSH_SCRIPT="$SCRIPTS_DIR/system/regenerate-ssh-key.sh"

# Execute random wallpaper script
if [ -f "$REGEN_SSH_SCRIPT" ]; then
    "$REGEN_SSH_SCRIPT" "$@"
else
    echo "Error: Regen SSH Script script not found at $REGEN_SSH_SCRIPT" >&2
    exit 1
fi

