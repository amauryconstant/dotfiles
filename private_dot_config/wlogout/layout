    "label" : "lock",
    "action" : "hyprlock",
    "text" : "󰌾 Lock",
    "keybind" : "l"
}
{
    "label" : "logout",
    "action" : "~/.local/lib/scripts/desktop/session-save && hyprctl dispatch exit",
    "text" : "󰍃 Logout",
    "keybind" : "e"
}
{
    "label" : "suspend",
    "action" : "systemctl suspend",
    "text" : "  Suspend",
    "keybind" : "u"
}
{
    "label" : "hibernate",
    "action" : "~/.local/lib/scripts/desktop/session-save hibernate && systemctl hibernate",
    "text" : "󰜗 Hibernate",
    "keybind" : "h"
}
{
    "label" : "reboot",
    "action" : "~/.local/lib/scripts/desktop/session-save reboot && systemctl reboot",
    "text" : "󰜉 Reboot",
    "keybind" : "r"
}
{
    "label" : "shutdown",
    "action" : "~/.local/lib/scripts/desktop/session-save shutdown && systemctl poweroff",
    "text" : "󰤂 Shutdown",
    "keybind" : "s"
}
