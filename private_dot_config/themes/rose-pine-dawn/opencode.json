{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-primary": "#faf4ed",
    "bg-secondary": "#fffaf3",
    "bg-tertiary": "#f2e9e1",
    "bg-overlay": "#fffaf3",
    "fg-primary": "#575279",
    "fg-secondary": "#797593",
    "fg-muted": "#9893a5",
    "fg-contrast": "#faf4ed",
    "accent-primary": "#907aa9",
    "accent-info": "#56949f",
    "accent-success": "#286983",
    "accent-warning": "#ea9d34",
    "accent-error": "#b4637a",
    "accent-highlight": "#907aa9",
    "accent-secondary": "#d7827e",
    "accent-tertiary": "#d7827e",
    "accent-modification": "#d7827e",
    "accent-border": "#907aa9",
    "accent-performance": "#b4637a",
    "accent-media": "#907aa9",
    "accent-subtle": "#9893a5",
    "accent-alternative": "#56949f",
    "accent-special": "#ea9d34",
    "accent-urgent-secondary": "#ea9d34"
  },
  "theme": {
    "primary": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "secondary": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "accent": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "error": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "warning": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "success": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "info": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "text": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "textMuted": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "background": {
      "dark": "bg-primary",
      "light": "bg-primary"
    },
    "backgroundPanel": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "backgroundElement": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "border": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderActive": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderSubtle": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "diffAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffContext": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHunkHeader": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHighlightAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffHighlightRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffAddedBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffRemovedBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffContextBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffLineNumber": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffAddedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffRemovedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "markdownText": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "markdownHeading": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownLink": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownLinkText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCode": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "markdownBlockQuote": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownEmph": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownStrong": {
      "dark": "accent-highlight",
      "light": "accent-highlight"
    },
    "markdownHorizontalRule": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownListItem": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownListEnumeration": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImage": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImageText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCodeBlock": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "syntaxComment": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "syntaxKeyword": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "syntaxFunction": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "syntaxVariable": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "syntaxString": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "syntaxNumber": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "syntaxType": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "syntaxOperator": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "syntaxPunctuation": {
      "dark": "fg-primary",
      "light": "fg-primary"
    }
  }
}
