{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-primary": "#232136",
    "bg-secondary": "#2a273f",
    "bg-tertiary": "#393552",
    "bg-overlay": "#393552",
    "fg-primary": "#e0def4",
    "fg-secondary": "#908caa",
    "fg-muted": "#6e6a86",
    "fg-contrast": "#232136",
    "accent-primary": "#c4a7e7",
    "accent-info": "#9ccfd8",
    "accent-success": "#3e8fb0",
    "accent-warning": "#f6c177",
    "accent-error": "#eb6f92",
    "accent-highlight": "#c4a7e7",
    "accent-secondary": "#ea9a97",
    "accent-tertiary": "#ea9a97",
    "accent-modification": "#ea9a97",
    "accent-border": "#c4a7e7",
    "accent-performance": "#eb6f92",
    "accent-media": "#c4a7e7",
    "accent-subtle": "#6e6a86",
    "accent-alternative": "#9ccfd8",
    "accent-special": "#f6c177",
    "accent-urgent-secondary": "#f6c177",
    "bg-diff-added": "#272a36",
    "bg-diff-removed": "#2f2a32",
    "bg-diff-context": "#2a273f",
    "fg-keyword": "#3e8fb0",
    "fg-code-block": "#e0def4",
    "accent-variable": "#ea9a97",
    "accent-type": "#c4a7e7",
    "accent-function-alt": "#9ccfd8",
    "accent-markdown-link": "#9ccfd8",
    "accent-markdown-code": "#3e8fb0",
    "accent-syntax-operator": "#ea9a97"
  },
  "theme": {
    "primary": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "secondary": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "accent": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "error": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "warning": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "success": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "info": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "text": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "textMuted": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "background": {
      "dark": "bg-primary",
      "light": "bg-primary"
    },
    "backgroundPanel": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "backgroundElement": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "border": {
      "dark": "accent-border",
      "light": "bg-tertiary"
    },
    "borderActive": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderSubtle": {
      "dark": "bg-tertiary",
      "light": "bg-secondary"
    },
    "diffAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffContext": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHunkHeader": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHighlightAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffHighlightRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffAddedBg": {
      "dark": "bg-diff-added",
      "light": "bg-diff-added"
    },
    "diffRemovedBg": {
      "dark": "bg-diff-removed",
      "light": "bg-diff-removed"
    },
    "diffContextBg": {
      "dark": "bg-diff-context",
      "light": "bg-diff-context"
    },
    "diffLineNumber": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffAddedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffRemovedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "markdownText": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "markdownHeading": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownLink": {
      "dark": "accent-markdown-link",
      "light": "accent-markdown-link"
    },
    "markdownLinkText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCode": {
      "dark": "accent-markdown-code",
      "light": "accent-markdown-code"
    },
    "markdownBlockQuote": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownEmph": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownStrong": {
      "dark": "accent-highlight",
      "light": "accent-highlight"
    },
    "markdownHorizontalRule": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownListItem": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownListEnumeration": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImage": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImageText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCodeBlock": {
      "dark": "fg-code-block",
      "light": "fg-code-block"
    },
    "syntaxComment": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "syntaxKeyword": {
      "dark": "fg-keyword",
      "light": "fg-keyword"
    },
    "syntaxFunction": {
      "dark": "accent-function-alt",
      "light": "accent-function-alt"
    },
    "syntaxVariable": {
      "dark": "accent-variable",
      "light": "accent-variable"
    },
    "syntaxString": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "syntaxNumber": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "syntaxType": {
      "dark": "accent-type",
      "light": "accent-type"
    },
    "syntaxOperator": {
      "dark": "accent-syntax-operator",
      "light": "accent-syntax-operator"
    },
    "syntaxPunctuation": {
      "dark": "fg-primary",
      "light": "fg-primary"
    }
  }
}
