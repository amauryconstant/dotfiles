{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-primary": "#eff1f5",
    "bg-secondary": "#e6e9ef",
    "bg-tertiary": "#dce0e8",
    "bg-overlay": "#e6e9ef",
    "bg-diff-added": "#d6f0d9",
    "bg-diff-removed": "#f6dfe2",
    "bg-diff-context": "#e6e9ef",
    "fg-primary": "#4c4f69",
    "fg-secondary": "#5c5f77",
    "fg-muted": "#7c7f93",
    "fg-subtle": "#6c6f85",
    "fg-contrast": "#dce0e8",
    "fg-keyword": "#8839ef",
    "fg-code-block": "#4c4f69",
    "accent-primary": "#1e66f5",
    "accent-info": "#179299",
    "accent-success": "#40a02b",
    "accent-warning": "#df8e1d",
    "accent-error": "#d20f39",
    "accent-highlight": "#8839ef",
    "accent-secondary": "#8839ef",
    "accent-tertiary": "#fe640b",
    "accent-modification": "#ea76cb",
    "accent-border": "#ccd0da",
    "accent-border-active": "#bcc0cc",
    "accent-border-subtle": "#acb0be",
    "accent-performance": "#e64553",
    "accent-media": "#8839ef",
    "accent-subtle": "#dc8a78",
    "accent-alternative": "#04a5e5",
    "accent-special": "#dd7878",
    "accent-urgent-secondary": "#fe640b",
    "accent-variable": "#d20f39",
    "accent-type": "#df8e1d",
    "accent-function-alt": "#1e66f5",
    "accent-markdown-link": "#04a5e5",
    "accent-markdown-code": "#40a02b",
    "accent-syntax-operator": "#04a5e5",
    "diff-line-number-bg-added": "#c9e3cb",
    "diff-line-number-bg-removed": "#e9d3d6"
  },
  "theme": {
    "primary": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "secondary": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "accent": {
      "dark": "accent-modification",
      "light": "accent-modification"
    },
    "error": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "warning": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "success": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "info": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "text": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "textMuted": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "background": {
      "dark": "bg-primary",
      "light": "bg-primary"
    },
    "backgroundPanel": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "backgroundElement": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "border": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderActive": {
      "dark": "accent-border-active",
      "light": "accent-border-active"
    },
    "borderSubtle": {
      "dark": "accent-border-subtle",
      "light": "accent-border-subtle"
    },
    "diffAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffContext": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHunkHeader": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "diffHighlightAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffHighlightRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffAddedBg": {
      "dark": "bg-diff-added",
      "light": "bg-diff-added"
    },
    "diffRemovedBg": {
      "dark": "bg-diff-removed",
      "light": "bg-diff-removed"
    },
    "diffContextBg": {
      "dark": "bg-diff-context",
      "light": "bg-diff-context"
    },
    "diffLineNumber": {
      "dark": "accent-border-active",
      "light": "accent-border-active"
    },
    "diffAddedLineNumberBg": {
      "dark": "diff-line-number-bg-added",
      "light": "diff-line-number-bg-added"
    },
    "diffRemovedLineNumberBg": {
      "dark": "diff-line-number-bg-removed",
      "light": "diff-line-number-bg-removed"
    },
    "markdownText": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "markdownHeading": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownLink": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownLinkText": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownCode": {
      "dark": "accent-markdown-code",
      "light": "accent-markdown-code"
    },
    "markdownBlockQuote": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownEmph": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownStrong": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "markdownHorizontalRule": {
      "dark": "fg-subtle",
      "light": "fg-subtle"
    },
    "markdownListItem": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownListEnumeration": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownImage": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownImageText": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownCodeBlock": {
      "dark": "fg-code-block",
      "light": "fg-code-block"
    },
    "syntaxComment": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "syntaxKeyword": {
      "dark": "fg-keyword",
      "light": "fg-keyword"
    },
    "syntaxFunction": {
      "dark": "accent-function-alt",
      "light": "accent-function-alt"
    },
    "syntaxVariable": {
      "dark": "accent-variable",
      "light": "accent-variable"
    },
    "syntaxString": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "syntaxNumber": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "syntaxType": {
      "dark": "accent-type",
      "light": "accent-type"
    },
    "syntaxOperator": {
      "dark": "accent-syntax-operator",
      "light": "accent-syntax-operator"
    },
    "syntaxPunctuation": {
      "dark": "fg-primary",
      "light": "fg-primary"
    }
  }
}
