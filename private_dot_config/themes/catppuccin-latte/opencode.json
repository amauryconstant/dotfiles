{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-primary": "#eff1f5",
    "bg-secondary": "#ccd0da",
    "bg-tertiary": "#bcc0cc",
    "bg-overlay": "#e6e9ef",
    "fg-primary": "#4c4f69",
    "fg-secondary": "#5c5f77",
    "fg-muted": "#7c7f93",
    "fg-contrast": "#dce0e8",
    "accent-primary": "#1e66f5",
    "accent-info": "#179299",
    "accent-success": "#40a02b",
    "accent-warning": "#df8e1d",
    "accent-error": "#d20f39",
    "accent-highlight": "#8839ef",
    "accent-secondary": "#209fb5",
    "accent-tertiary": "#fe640b",
    "accent-modification": "#ea76cb",
    "accent-border": "#7287fd",
    "accent-performance": "#e64553",
    "accent-media": "#8839ef",
    "accent-subtle": "#dc8a78",
    "accent-alternative": "#04a5e5",
    "accent-special": "#dd7878",
    "accent-urgent-secondary": "#fe640b"
  },
  "theme": {
    "primary": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "secondary": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "accent": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "error": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "warning": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "success": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "info": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "text": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "textMuted": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "background": {
      "dark": "bg-primary",
      "light": "bg-primary"
    },
    "backgroundPanel": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "backgroundElement": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "border": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderActive": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderSubtle": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "diffAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffContext": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHunkHeader": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHighlightAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffHighlightRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffAddedBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffRemovedBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffContextBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffLineNumber": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffAddedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffRemovedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "markdownText": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "markdownHeading": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownLink": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownLinkText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCode": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "markdownBlockQuote": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownEmph": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownStrong": {
      "dark": "accent-highlight",
      "light": "accent-highlight"
    },
    "markdownHorizontalRule": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownListItem": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownListEnumeration": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImage": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImageText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCodeBlock": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "syntaxComment": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "syntaxKeyword": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "syntaxFunction": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "syntaxVariable": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "syntaxString": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "syntaxNumber": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "syntaxType": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "syntaxOperator": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "syntaxPunctuation": {
      "dark": "fg-primary",
      "light": "fg-primary"
    }
  }
}
