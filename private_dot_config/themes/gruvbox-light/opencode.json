{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-primary": "#fbf1c7",
    "bg-secondary": "#ebdbb2",
    "bg-tertiary": "#d5c4a1",
    "bg-overlay": "#ebdbb2",
    "bg-diff-added": "#ebf3cd",
    "bg-diff-removed": "#f9e2d2",
    "bg-diff-context": "#ebdbb2",
    "fg-primary": "#3c3836",
    "fg-secondary": "#3c3836",
    "fg-muted": "#7c6f64",
    "fg-subtle": "#bdae93",
    "fg-contrast": "#282828",
    "fg-keyword": "#8f3f71",
    "fg-code-block": "#3c3836",
    "accent-primary": "#076678",
    "accent-info": "#689d6a",
    "accent-success": "#98971a",
    "accent-warning": "#d79921",
    "accent-error": "#cc241d",
    "accent-highlight": "#8f3f71",
    "accent-secondary": "#8f3f71",
    "accent-tertiary": "#af3a03",
    "accent-modification": "#af3a03",
    "accent-border": "#bdae93",
    "accent-border-active": "#a89984",
    "accent-border-subtle": "#d5c4a1",
    "accent-performance": "#b16286",
    "accent-media": "#8f3f71",
    "accent-subtle": "#928374",
    "accent-alternative": "#689d6a",
    "accent-special": "#af3a03",
    "accent-urgent-secondary": "#d65d0e",
    "accent-variable": "#8f3f71",
    "accent-type": "#d79921",
    "accent-function-alt": "#076678",
    "accent-markdown-link": "#076678",
    "accent-markdown-code": "#98971a",
    "accent-syntax-operator": "#689d6a",
    "diff-line-number-bg-added": "#e5edc8",
    "diff-line-number-bg-removed": "#f4ddd2"
  },
  "theme": {
    "primary": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "secondary": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "accent": {
      "dark": "accent-modification",
      "light": "accent-modification"
    },
    "error": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "warning": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "success": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "info": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "text": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "textMuted": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "background": {
      "dark": "bg-primary",
      "light": "bg-primary"
    },
    "backgroundPanel": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "backgroundElement": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "border": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderActive": {
      "dark": "accent-border-active",
      "light": "accent-border-active"
    },
    "borderSubtle": {
      "dark": "accent-border-subtle",
      "light": "accent-border-subtle"
    },
    "diffAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffContext": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHunkHeader": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "diffHighlightAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffHighlightRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffAddedBg": {
      "dark": "bg-diff-added",
      "light": "bg-diff-added"
    },
    "diffRemovedBg": {
      "dark": "bg-diff-removed",
      "light": "bg-diff-removed"
    },
    "diffContextBg": {
      "dark": "bg-diff-context",
      "light": "bg-diff-context"
    },
    "diffLineNumber": {
      "dark": "accent-border-active",
      "light": "accent-border-active"
    },
    "diffAddedLineNumberBg": {
      "dark": "diff-line-number-bg-added",
      "light": "diff-line-number-bg-added"
    },
    "diffRemovedLineNumberBg": {
      "dark": "diff-line-number-bg-removed",
      "light": "diff-line-number-bg-removed"
    },
    "markdownText": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "markdownHeading": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownLink": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownLinkText": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownCode": {
      "dark": "accent-markdown-code",
      "light": "accent-markdown-code"
    },
    "markdownBlockQuote": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownEmph": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownStrong": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "markdownHorizontalRule": {
      "dark": "fg-subtle",
      "light": "fg-subtle"
    },
    "markdownListItem": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownListEnumeration": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownImage": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownImageText": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownCodeBlock": {
      "dark": "fg-code-block",
      "light": "fg-code-block"
    },
    "syntaxComment": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "syntaxKeyword": {
      "dark": "fg-keyword",
      "light": "fg-keyword"
    },
    "syntaxFunction": {
      "dark": "accent-function-alt",
      "light": "accent-function-alt"
    },
    "syntaxVariable": {
      "dark": "accent-variable",
      "light": "accent-variable"
    },
    "syntaxString": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "syntaxNumber": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "syntaxType": {
      "dark": "accent-type",
      "light": "accent-type"
    },
    "syntaxOperator": {
      "dark": "accent-syntax-operator",
      "light": "accent-syntax-operator"
    },
    "syntaxPunctuation": {
      "dark": "fg-primary",
      "light": "fg-primary"
    }
  }
}
