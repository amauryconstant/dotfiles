{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-primary": "#282828",
    "bg-secondary": "#3c3836",
    "bg-tertiary": "#504945",
    "bg-overlay": "#3c3836",
    "fg-primary": "#ebdbb2",
    "fg-secondary": "#d5c4a1",
    "fg-muted": "#a89984",
    "fg-contrast": "#fbf1c7",
    "accent-primary": "#fabd2f",
    "accent-info": "#8ec07c",
    "accent-success": "#b8bb26",
    "accent-warning": "#fabd2f",
    "accent-error": "#fb4934",
    "accent-highlight": "#d3869b",
    "accent-secondary": "#8ec07c",
    "accent-tertiary": "#fe8019",
    "accent-modification": "#fe8019",
    "accent-border": "#fabd2f",
    "accent-performance": "#d3869b",
    "accent-media": "#d3869b",
    "accent-subtle": "#928374",
    "accent-alternative": "#8ec07c",
    "accent-special": "#fe8019",
    "accent-urgent-secondary": "#fe8019"
  },
  "theme": {
    "primary": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "secondary": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "accent": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "error": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "warning": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "success": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "info": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "text": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "textMuted": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "background": {
      "dark": "bg-primary",
      "light": "bg-primary"
    },
    "backgroundPanel": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "backgroundElement": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "border": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderActive": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderSubtle": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "diffAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffContext": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHunkHeader": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHighlightAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffHighlightRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffAddedBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffRemovedBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffContextBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffLineNumber": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffAddedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffRemovedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "markdownText": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "markdownHeading": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownLink": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownLinkText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCode": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "markdownBlockQuote": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownEmph": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownStrong": {
      "dark": "accent-highlight",
      "light": "accent-highlight"
    },
    "markdownHorizontalRule": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownListItem": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownListEnumeration": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImage": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImageText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCodeBlock": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "syntaxComment": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "syntaxKeyword": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "syntaxFunction": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "syntaxVariable": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "syntaxString": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "syntaxNumber": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "syntaxType": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "syntaxOperator": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "syntaxPunctuation": {
      "dark": "fg-primary",
      "light": "fg-primary"
    }
  }
}
