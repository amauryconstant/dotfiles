{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-primary": "#282828",
    "bg-secondary": "#3c3836",
    "bg-tertiary": "#504945",
    "bg-overlay": "#3c3836",
    "bg-diff-added": "#32302f",
    "bg-diff-removed": "#3c2929",
    "bg-diff-context": "#3c3836",
    "fg-primary": "#ebdbb2",
    "fg-secondary": "#ebdbb2",
    "fg-muted": "#928374",
    "fg-subtle": "#665c54",
    "fg-contrast": "#fbf1c7",
    "fg-keyword": "#d3869b",
    "fg-code-block": "#ebdbb2",
    "accent-primary": "#83a598",
    "accent-info": "#8ec07c",
    "accent-success": "#b8bb26",
    "accent-warning": "#fabd2f",
    "accent-error": "#fb4934",
    "accent-highlight": "#d3869b",
    "accent-secondary": "#d3869b",
    "accent-tertiary": "#fe8019",
    "accent-modification": "#fe8019",
    "accent-border": "#665c54",
    "accent-border-active": "#7c6f64",
    "accent-border-subtle": "#504945",
    "accent-performance": "#d3869b",
    "accent-media": "#d3869b",
    "accent-subtle": "#928374",
    "accent-alternative": "#8ec07c",
    "accent-special": "#fe8019",
    "accent-urgent-secondary": "#fe8019",
    "accent-variable": "#d3869b",
    "accent-type": "#fabd2f",
    "accent-function-alt": "#83a598",
    "accent-markdown-link": "#83a598",
    "accent-markdown-code": "#b8bb26",
    "accent-syntax-operator": "#8ec07c",
    "diff-line-number-bg-added": "#2a2827",
    "diff-line-number-bg-removed": "#342525"
  },
  "theme": {
    "primary": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "secondary": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "accent": {
      "dark": "accent-modification",
      "light": "accent-modification"
    },
    "error": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "warning": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "success": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "info": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "text": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "textMuted": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "background": {
      "dark": "bg-primary",
      "light": "bg-primary"
    },
    "backgroundPanel": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "backgroundElement": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "border": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderActive": {
      "dark": "accent-border-active",
      "light": "accent-border-active"
    },
    "borderSubtle": {
      "dark": "accent-border-subtle",
      "light": "accent-border-subtle"
    },
    "diffAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffContext": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHunkHeader": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "diffHighlightAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffHighlightRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffAddedBg": {
      "dark": "bg-diff-added",
      "light": "bg-diff-added"
    },
    "diffRemovedBg": {
      "dark": "bg-diff-removed",
      "light": "bg-diff-removed"
    },
    "diffContextBg": {
      "dark": "bg-diff-context",
      "light": "bg-diff-context"
    },
    "diffLineNumber": {
      "dark": "accent-border-active",
      "light": "accent-border-active"
    },
    "diffAddedLineNumberBg": {
      "dark": "diff-line-number-bg-added",
      "light": "diff-line-number-bg-added"
    },
    "diffRemovedLineNumberBg": {
      "dark": "diff-line-number-bg-removed",
      "light": "diff-line-number-bg-removed"
    },
    "markdownText": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "markdownHeading": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownLink": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownLinkText": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownCode": {
      "dark": "accent-markdown-code",
      "light": "accent-markdown-code"
    },
    "markdownBlockQuote": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownEmph": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownStrong": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "markdownHorizontalRule": {
      "dark": "fg-subtle",
      "light": "fg-subtle"
    },
    "markdownListItem": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownListEnumeration": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownImage": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownImageText": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownCodeBlock": {
      "dark": "fg-code-block",
      "light": "fg-code-block"
    },
    "syntaxComment": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "syntaxKeyword": {
      "dark": "fg-keyword",
      "light": "fg-keyword"
    },
    "syntaxFunction": {
      "dark": "accent-function-alt",
      "light": "accent-function-alt"
    },
    "syntaxVariable": {
      "dark": "accent-variable",
      "light": "accent-variable"
    },
    "syntaxString": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "syntaxNumber": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "syntaxType": {
      "dark": "accent-type",
      "light": "accent-type"
    },
    "syntaxOperator": {
      "dark": "accent-syntax-operator",
      "light": "accent-syntax-operator"
    },
    "syntaxPunctuation": {
      "dark": "fg-primary",
      "light": "fg-primary"
    }
  }
}
