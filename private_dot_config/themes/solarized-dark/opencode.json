{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-primary": "#002b36",
    "bg-secondary": "#073642",
    "bg-tertiary": "#586e75",
    "bg-overlay": "#073642",
    "fg-primary": "#839496",
    "fg-secondary": "#93a1a1",
    "fg-muted": "#586e75",
    "fg-contrast": "#fdf6e3",
    "accent-primary": "#268bd2",
    "accent-info": "#2aa198",
    "accent-success": "#859900",
    "accent-warning": "#b58900",
    "accent-error": "#dc322f",
    "accent-highlight": "#6c71c4",
    "accent-secondary": "#2aa198",
    "accent-tertiary": "#cb4b16",
    "accent-modification": "#d33682",
    "accent-border": "#6c71c4",
    "accent-performance": "#d33682",
    "accent-media": "#6c71c4",
    "accent-subtle": "#586e75",
    "accent-alternative": "#2aa198",
    "accent-special": "#cb4b16",
    "accent-urgent-secondary": "#cb4b16"
  },
  "theme": {
    "primary": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "secondary": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "accent": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "error": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "warning": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "success": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "info": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "text": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "textMuted": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "background": {
      "dark": "bg-primary",
      "light": "bg-primary"
    },
    "backgroundPanel": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "backgroundElement": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "border": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderActive": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderSubtle": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "diffAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffContext": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHunkHeader": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHighlightAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffHighlightRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffAddedBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffRemovedBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffContextBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffLineNumber": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffAddedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffRemovedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "markdownText": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "markdownHeading": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownLink": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownLinkText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCode": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "markdownBlockQuote": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownEmph": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownStrong": {
      "dark": "accent-highlight",
      "light": "accent-highlight"
    },
    "markdownHorizontalRule": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownListItem": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownListEnumeration": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImage": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImageText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCodeBlock": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "syntaxComment": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "syntaxKeyword": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "syntaxFunction": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "syntaxVariable": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "syntaxString": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "syntaxNumber": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "syntaxType": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "syntaxOperator": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "syntaxPunctuation": {
      "dark": "fg-primary",
      "light": "fg-primary"
    }
  }
}
