{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-primary": "#002b36",
    "bg-secondary": "#073642",
    "bg-tertiary": "#073642",
    "bg-overlay": "#073642",
    "bg-diff-added": "#073642",
    "bg-diff-removed": "#073642",
    "bg-diff-context": "#073642",
    "fg-primary": "#839496",
    "fg-secondary": "#586e75",
    "fg-muted": "#586e75",
    "fg-subtle": "#586e75",
    "fg-contrast": "#fdf6e3",
    "fg-keyword": "#268bd2",
    "fg-code-block": "#839496",
    "accent-primary": "#268bd2",
    "accent-info": "#2aa198",
    "accent-success": "#859900",
    "accent-warning": "#b58900",
    "accent-error": "#dc322f",
    "accent-highlight": "#6c71c4",
    "accent-secondary": "#6c71c4",
    "accent-tertiary": "#cb4b16",
    "accent-modification": "#d33682",
    "accent-border": "#073642",
    "accent-border-active": "#586e75",
    "accent-border-subtle": "#073642",
    "accent-performance": "#d33682",
    "accent-media": "#6c71c4",
    "accent-subtle": "#586e75",
    "accent-alternative": "#2aa198",
    "accent-special": "#cb4b16",
    "accent-urgent-secondary": "#cb4b16",
    "accent-variable": "#6c71c4",
    "accent-type": "#b58900",
    "accent-function-alt": "#268bd2",
    "accent-markdown-link": "#268bd2",
    "accent-markdown-code": "#859900",
    "accent-syntax-operator": "#2aa198",
    "diff-line-number-bg-added": "#073642",
    "diff-line-number-bg-removed": "#073642"
  },
  "theme": {
    "primary": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "secondary": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "accent": {
      "dark": "accent-modification",
      "light": "accent-modification"
    },
    "error": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "warning": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "success": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "info": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "text": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "textMuted": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "background": {
      "dark": "bg-primary",
      "light": "bg-primary"
    },
    "backgroundPanel": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "backgroundElement": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "border": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderActive": {
      "dark": "accent-border-active",
      "light": "accent-border-active"
    },
    "borderSubtle": {
      "dark": "accent-border-subtle",
      "light": "accent-border-subtle"
    },
    "diffAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffContext": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHunkHeader": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "diffHighlightAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffHighlightRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffAddedBg": {
      "dark": "bg-diff-added",
      "light": "bg-diff-added"
    },
    "diffRemovedBg": {
      "dark": "bg-diff-removed",
      "light": "bg-diff-removed"
    },
    "diffContextBg": {
      "dark": "bg-diff-context",
      "light": "bg-diff-context"
    },
    "diffLineNumber": {
      "dark": "accent-border-active",
      "light": "accent-border-active"
    },
    "diffAddedLineNumberBg": {
      "dark": "diff-line-number-bg-added",
      "light": "diff-line-number-bg-added"
    },
    "diffRemovedLineNumberBg": {
      "dark": "diff-line-number-bg-removed",
      "light": "diff-line-number-bg-removed"
    },
    "markdownText": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "markdownHeading": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownLink": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownLinkText": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownCode": {
      "dark": "accent-markdown-code",
      "light": "accent-markdown-code"
    },
    "markdownBlockQuote": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownEmph": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownStrong": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "markdownHorizontalRule": {
      "dark": "fg-subtle",
      "light": "fg-subtle"
    },
    "markdownListItem": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownListEnumeration": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownImage": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownImageText": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownCodeBlock": {
      "dark": "fg-code-block",
      "light": "fg-code-block"
    },
    "syntaxComment": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "syntaxKeyword": {
      "dark": "fg-keyword",
      "light": "fg-keyword"
    },
    "syntaxFunction": {
      "dark": "accent-function-alt",
      "light": "accent-function-alt"
    },
    "syntaxVariable": {
      "dark": "accent-variable",
      "light": "accent-variable"
    },
    "syntaxString": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "syntaxNumber": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "syntaxType": {
      "dark": "accent-type",
      "light": "accent-type"
    },
    "syntaxOperator": {
      "dark": "accent-syntax-operator",
      "light": "accent-syntax-operator"
    },
    "syntaxPunctuation": {
      "dark": "fg-primary",
      "light": "fg-primary"
    }
  }
}
