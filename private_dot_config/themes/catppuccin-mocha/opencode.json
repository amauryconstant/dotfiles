{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-primary": "#1e1e2e",
    "bg-secondary": "#313244",
    "bg-tertiary": "#45475a",
    "bg-overlay": "#181825",
    "bg-diff-added": "#2d353b",
    "bg-diff-removed": "#3b2f32",
    "bg-diff-context": "#313244",
    "fg-primary": "#cdd6f4",
    "fg-secondary": "#bac2de",
    "fg-muted": "#9399b2",
    "fg-contrast": "#11111b",
    "fg-keyword": "#cba6f7",
    "fg-code-block": "#cdd6f4",
    "accent-primary": "#89b4fa",
    "accent-info": "#94e2d5",
    "accent-success": "#a6e3a1",
    "accent-warning": "#f9e2af",
    "accent-error": "#f38ba8",
    "accent-highlight": "#cba6f7",
    "accent-secondary": "#74c7ec",
    "accent-tertiary": "#fab387",
    "accent-modification": "#f5c2e7",
    "accent-border": "#b4befe",
    "accent-performance": "#eba0ac",
    "accent-media": "#cba6f7",
    "accent-subtle": "#f5e0dc",
    "accent-alternative": "#89dceb",
    "accent-special": "#f2cdcd",
    "accent-urgent-secondary": "#fab387",
    "accent-variable": "#f2cdcd",
    "accent-type": "#f9e2af",
    "accent-function-alt": "#74c7ec",
    "accent-markdown-link": "#89dceb",
    "accent-markdown-code": "#94e2d5",
    "accent-syntax-operator": "#74c7ec"
  },
  "theme": {
    "primary": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "secondary": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "accent": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "error": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "warning": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "success": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "info": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "text": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "textMuted": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "background": {
      "dark": "bg-primary",
      "light": "bg-primary"
    },
    "backgroundPanel": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "backgroundElement": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "border": {
      "dark": "accent-border",
      "light": "bg-tertiary"
    },
    "borderActive": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderSubtle": {
      "dark": "bg-tertiary",
      "light": "bg-secondary"
    },
    "diffAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffContext": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHunkHeader": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHighlightAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffHighlightRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffAddedBg": {
      "dark": "bg-diff-added",
      "light": "bg-diff-added"
    },
    "diffRemovedBg": {
      "dark": "bg-diff-removed",
      "light": "bg-diff-removed"
    },
    "diffContextBg": {
      "dark": "bg-diff-context",
      "light": "bg-diff-context"
    },
    "diffLineNumber": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffAddedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "diffRemovedLineNumberBg": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "markdownText": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "markdownHeading": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownLink": {
      "dark": "accent-markdown-link",
      "light": "accent-markdown-link"
    },
    "markdownLinkText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCode": {
      "dark": "accent-markdown-code",
      "light": "accent-markdown-code"
    },
    "markdownBlockQuote": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownEmph": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownStrong": {
      "dark": "accent-highlight",
      "light": "accent-highlight"
    },
    "markdownHorizontalRule": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "markdownListItem": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownListEnumeration": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImage": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "markdownImageText": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownCodeBlock": {
      "dark": "fg-code-block",
      "light": "fg-code-block"
    },
    "syntaxComment": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "syntaxKeyword": {
      "dark": "fg-keyword",
      "light": "fg-keyword"
    },
    "syntaxFunction": {
      "dark": "accent-function-alt",
      "light": "accent-function-alt"
    },
    "syntaxVariable": {
      "dark": "accent-variable",
      "light": "accent-variable"
    },
    "syntaxString": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "syntaxNumber": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "syntaxType": {
      "dark": "accent-type",
      "light": "accent-type"
    },
    "syntaxOperator": {
      "dark": "accent-syntax-operator",
      "light": "accent-syntax-operator"
    },
    "syntaxPunctuation": {
      "dark": "fg-primary",
      "light": "fg-primary"
    }
  }
}
