{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-primary": "#1e1e2e",
    "bg-secondary": "#181825",
    "bg-tertiary": "#11111b",
    "bg-overlay": "#181825",
    "bg-diff-added": "#24312b",
    "bg-diff-removed": "#3c2a32",
    "bg-diff-context": "#181825",
    "fg-primary": "#cdd6f4",
    "fg-secondary": "#bac2de",
    "fg-muted": "#9399b2",
    "fg-subtle": "#a6adc8",
    "fg-contrast": "#11111b",
    "fg-keyword": "#cba6f7",
    "fg-code-block": "#cdd6f4",
    "accent-primary": "#89b4fa",
    "accent-info": "#94e2d5",
    "accent-success": "#a6e3a1",
    "accent-warning": "#f9e2af",
    "accent-error": "#f38ba8",
    "accent-highlight": "#cba6f7",
    "accent-secondary": "#cba6f7",
    "accent-tertiary": "#fab387",
    "accent-modification": "#f5c2e7",
    "accent-border": "#313244",
    "accent-border-active": "#45475a",
    "accent-border-subtle": "#585b70",
    "accent-performance": "#eba0ac",
    "accent-media": "#cba6f7",
    "accent-subtle": "#f5e0dc",
    "accent-alternative": "#89dceb",
    "accent-special": "#f2cdcd",
    "accent-urgent-secondary": "#fab387",
    "accent-variable": "#f38ba8",
    "accent-type": "#f9e2af",
    "accent-function-alt": "#89b4fa",
    "accent-markdown-link": "#89dceb",
    "accent-markdown-code": "#a6e3a1",
    "accent-syntax-operator": "#89dceb",
    "diff-line-number-bg-added": "#1e2a25",
    "diff-line-number-bg-removed": "#32232a"
  },
  "theme": {
    "primary": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "secondary": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "accent": {
      "dark": "accent-modification",
      "light": "accent-modification"
    },
    "error": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "warning": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "success": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "info": {
      "dark": "accent-info",
      "light": "accent-info"
    },
    "text": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "textMuted": {
      "dark": "fg-secondary",
      "light": "fg-secondary"
    },
    "background": {
      "dark": "bg-primary",
      "light": "bg-primary"
    },
    "backgroundPanel": {
      "dark": "bg-secondary",
      "light": "bg-secondary"
    },
    "backgroundElement": {
      "dark": "bg-tertiary",
      "light": "bg-tertiary"
    },
    "border": {
      "dark": "accent-border",
      "light": "accent-border"
    },
    "borderActive": {
      "dark": "accent-border-active",
      "light": "accent-border-active"
    },
    "borderSubtle": {
      "dark": "accent-border-subtle",
      "light": "accent-border-subtle"
    },
    "diffAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffContext": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "diffHunkHeader": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "diffHighlightAdded": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "diffHighlightRemoved": {
      "dark": "accent-error",
      "light": "accent-error"
    },
    "diffAddedBg": {
      "dark": "bg-diff-added",
      "light": "bg-diff-added"
    },
    "diffRemovedBg": {
      "dark": "bg-diff-removed",
      "light": "bg-diff-removed"
    },
    "diffContextBg": {
      "dark": "bg-diff-context",
      "light": "bg-diff-context"
    },
    "diffLineNumber": {
      "dark": "accent-border-active",
      "light": "accent-border-active"
    },
    "diffAddedLineNumberBg": {
      "dark": "diff-line-number-bg-added",
      "light": "diff-line-number-bg-added"
    },
    "diffRemovedLineNumberBg": {
      "dark": "diff-line-number-bg-removed",
      "light": "diff-line-number-bg-removed"
    },
    "markdownText": {
      "dark": "fg-primary",
      "light": "fg-primary"
    },
    "markdownHeading": {
      "dark": "accent-secondary",
      "light": "accent-secondary"
    },
    "markdownLink": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownLinkText": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownCode": {
      "dark": "accent-markdown-code",
      "light": "accent-markdown-code"
    },
    "markdownBlockQuote": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownEmph": {
      "dark": "accent-warning",
      "light": "accent-warning"
    },
    "markdownStrong": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "markdownHorizontalRule": {
      "dark": "fg-subtle",
      "light": "fg-subtle"
    },
    "markdownListItem": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownListEnumeration": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownImage": {
      "dark": "accent-primary",
      "light": "accent-primary"
    },
    "markdownImageText": {
      "dark": "accent-alternative",
      "light": "accent-alternative"
    },
    "markdownCodeBlock": {
      "dark": "fg-code-block",
      "light": "fg-code-block"
    },
    "syntaxComment": {
      "dark": "fg-muted",
      "light": "fg-muted"
    },
    "syntaxKeyword": {
      "dark": "fg-keyword",
      "light": "fg-keyword"
    },
    "syntaxFunction": {
      "dark": "accent-function-alt",
      "light": "accent-function-alt"
    },
    "syntaxVariable": {
      "dark": "accent-variable",
      "light": "accent-variable"
    },
    "syntaxString": {
      "dark": "accent-success",
      "light": "accent-success"
    },
    "syntaxNumber": {
      "dark": "accent-tertiary",
      "light": "accent-tertiary"
    },
    "syntaxType": {
      "dark": "accent-type",
      "light": "accent-type"
    },
    "syntaxOperator": {
      "dark": "accent-syntax-operator",
      "light": "accent-syntax-operator"
    },
    "syntaxPunctuation": {
      "dark": "fg-primary",
      "light": "fg-primary"
    }
  }
}
